<UserControl x:Class="RoboticsPos.UI.DiscontList"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:RoboticsPos.UI"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="600">
    <UserControl.Resources>
        <ResourceDictionary Source="/Controls.xaml"/>
    </UserControl.Resources>
    <Grid>
        <StackPanel>
            <Button Style="{DynamicResource RoundButton}" Background="Blue" Content="Create" Foreground="White" FontSize="15" Width="120" Height="35" HorizontalAlignment="Left" Margin="10 10 0 0"></Button>
            
            
        </StackPanel>
    </Grid>
</UserControl>
